      "source": [
        "#hide_input\n",
        "\n",
        "df = pd.read_html('https://en.wikipedia.org/wiki/List_of_production_battery_electric_vehicles_(table)')[2]\n",
        "\n",
        "def extract_start(s):\n",
        "  return re.findall(r\"[-+]?\\d*\\,\\d+|\\d+\",s)[0].replace(',','')\n",
        "\n",
        "def extract_end(s):\n",
        "  try:\n",
        "    return re.findall(\"\\d+\",s)[1]\n",
        "  except:\n",
        "    return 2020\n",
        "\n",
        "df['Start Production'] = df['Production years'].dropna().apply(extract_start).astype(str)\n",
        "df['End Production'] = df['Production years'].dropna().apply(extract_end).astype(str)\n",
        "df['Sold'] = df['Number produced/sold (less than or estimate)'].dropna().apply(extract_start).astype(float)\n",
        "px.timeline(df[['Name','Start Production','End Production','Sold']].dropna(), x_start=\"Start Production\", x_end=\"End Production\", y=\"Name\", color=\"Sold\",title='Timeline of EV sales on the global market')"
      ],
      "execution_count": null,
